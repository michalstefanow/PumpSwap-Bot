    "sell": "ts-node src/cli/sell-cli.ts",
    "price": "ts-node src/cli/buy-cli.ts price",
    "pool": "ts-node src/cli/buy-cli.ts pool",
    "balance": "ts-node src/cli/sell-cli.ts balance"
  },
